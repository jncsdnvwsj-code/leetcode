    "def runningSum(nums):\n",
    "        A = [nums[0]]\n",
    "        for i in range(1,len(nums)):\n",
    "            A.append(nums[i] + A[i-1])\n",
    "        return A\n",
    "\n",
    "runningSum([3,1,2,10,1])"
   ]
